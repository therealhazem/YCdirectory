{
  "name": "yc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:canary": "cross-env NEXT_CANARY=1 next dev --turbopack",
    "build": "next build --turbopack",
    "build:canary": "cross-env NEXT_CANARY=1 next build --turbopack",
    "start": "next start",
    "start:canary": "cross-env NEXT_CANARY=1 next start",
